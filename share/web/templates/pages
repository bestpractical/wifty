<%args>
$pages
</%args>
<&|/_elements/wrapper, title => 'These are the pages on your wiki!' &>
<ul id="pagelist">
% while (my $page = $pages->next) {
<li><% 
        Jifty->web->link(
            label => $page->name,
            url   => '/view/' . $page->name
            );

    %></li>
% } 
</ul>
</&>
